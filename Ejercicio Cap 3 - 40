Ejercicio 40
## Calcular el area superficial de S de un prisma con dimensiones l,h,w. En este problema, l=10, h=4 y w=5.2m.

#include <iostream>

int main () {

      doble l=10,h=4,w=5.2;
			doble m;
			std :: cout << "l (largo):" << l << "\ n";
			std :: cout << "h (alto):" << h << "\ n";
			std :: cout << "w (ancho):" << w << "\ n";
			S = l*h*w;
			std :: cout << "El area superficial es de :" << S << "m." << std :: endl;
    return 0;
}
